{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "allowCompilationErrors": true,
      "outputFormat": "markdown"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "./templates/inheritance-collapsed"
    ],
    "globalMetadata": {
      "_appName": "Home",
      "_appTitle": "Pgaing",
      "_enableSearch": true,
      "_appFooter": "<span>Copyright Â© 2024 Qrtix <strong>Carlos J. Ortiz</strong></span>",
      "pdf": false
    }
  }
}